<!DOCTYPE html>
<html lang="en">
    <%- include("head.ejs") -%>
<body>
    <%- include("header.ejs") -%>
    <main>
        <div class="wrapper">
            <% products.forEach((products)=> {-%>
                <div class="ads">
                    <h3>
                        <%= products.title %>
                    </h3>
                    <p>
                        <%= products.desc %>
                    </p>
                </div>
               <% }) -%> 
        </div>
        <%- include("modal.ejs") -%>
    </main>
    <%- include ("footer.ejs") -%>
</body>
</html>